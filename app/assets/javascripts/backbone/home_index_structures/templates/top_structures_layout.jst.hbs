<div data-type="loader" class="home-index-structures__loader" style="display: none;">
  <img src="/gifs/loading-indicator.gif"/ class="center-block" height="10">
</div>
<section id="widgets-container" class="home-index-structures__map-wrapper relative" data-type="google-maps">
    {{#each widget}}
        <div id="{{ id }}" data-type="widget"></div>
    {{/each}}
</section>

<section class="home-index-structures__list-wrapper">
    <header class="flexbox page-entries-wrapper soft-half">
        <div class="one-whole" data-type="insert-widget-slug"></div>
    </header>

    <div id="top-structures">
    </div>
    <div class="home-index-structures__infinite-scroll text--center" data-type="infinite-scroll-button"></div>

</section>

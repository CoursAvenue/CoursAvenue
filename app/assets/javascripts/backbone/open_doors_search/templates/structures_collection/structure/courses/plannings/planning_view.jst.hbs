<td class="three-twelfths nowrap">{{ date }} de {{ time_slot }} ({{ duration }})</td>
<td class="two-twelfths">{{ audiences }}</td>
<td class="two-twelfths">{{ levels }}</td>
<td class="two-twelfths nowrap text--center">
    {{#if more_than_ten_places}}
    > 10
    {{else}}
        {{#ifCond open_courses_open_places '==' '0'}}
            Liste d'attente
        {{else}}
            < 10
        {{/ifCond}}
    {{/if}}
</td>
<td class="two-twelfths nowrap">
    {{#if common_price}}
    <span class="line-through">{{common_price}}€</span>
    {{/if}}
    <span class="green">GRATUIT</span>
</td>
<td class="one-twelfths text--right">
    <a href='{{registration_url}}' data-behavior="modal"  data-title='Inscription au créneau' data-width="600" class='btn btn--green btn--small fancybox.ajax' data-target="participate">
    {{#ifCond open_courses_open_places '==' '0'}}
        M'inscrire en liste d'attente
    {{else}}
        M'inscrire
    {{/ifCond}}
    </a>
</td>

.lbc-icon-calendar, .lbc-icon-clock {
    display          : inline-block;
    background-size  : 100%;
    background-repeat: no-repeat;
    width            : 14px;
    height           : 14px;
    vertical-align   : middle;
    position         : relative;
    top              : -1px;
}

.lbc-icon-calendar {
    background-image : url(image_path('icons/calendar.png'));
    height           : 16px;
}
.lbc-icon-clock {
    background-image : url(image_path('icons/clock.png'));
}

<% if @participation.persisted? %>
    $.fancybox.close();
    window.location.href = "<%= user_participations_url(current_user, inscription_confirmed: true) %>";
<% else %>
    $.fancybox("<%= escape_javascript(render partial: 'plannings/participations/form') %>", { width: 600, maxWidth: 600 });
<% end %>

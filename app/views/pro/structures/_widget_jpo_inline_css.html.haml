:css
  .coursavenue-jpo-widget {
    font-family          : Helvetica, Arial,sans-serif !important;
    font-size            : 12px !important;
    max-width            : 750px !important;
    margin               : auto !important;
    margin-bottom        : 11px !important;
    background-color     : #ea724b !important;
    -webkit-border-radius: 2px !important;
    -moz-border-radius   : 2px !important;
    -ms-border-radius    : 2px !important;
    -o-border-radius     : 2px !important;
    border-radius        : 2px !important;
    padding              : 10px!important;
  }
  .coursavenue-jpo-widget p {
    margin: 0!important;
  }
  .coursavenue-jpo-recommanded {
    width    : 70% !important;
    max-width: 250px !important;
  }
  .coursavenue-jpo-stars {
    width    : 40% !important;
    max-width: 150px !important;
  }
  .coursavenue-jpo-logo {
    width    : 90% !important;
    max-width: 350px !important;
  }
  .coursavenue-jpo-recommanded, .coursavenue-jpo-stars, .coursavenue-jpo-logo {
    background: none !important;
    border    : none !important;
    padding   : 5px 0 !important;
    box-shadow: none !important;
  }
  .coursavenue-jpo-widget-wrapper-link {
    display        : block !important;
    padding        : 10px !important;
    margin         : 0 auto 10px auto !important;
    text-align     : center !important;
    border         : 1px solid #DDD !important;
    text-decoration: none !important;
  }
  .coursavenue-jpo-widget-title {
    font-size: 16px !important;
    color    : #e9683d !important;
    display  : block!important;
  }
  .coursavenue-jpo-widget-title img {
    display  : block!important;
    margin   : auto!important;
  }
  .coursavenue-jpo-link {
    background-color: blue !important;
  }
  .coursavenue-jpo-white-box {
    display               : block !important;
    font-family           : Helvetica, Arial,sans-serif !important;
    padding               : 10px !important;
    margin                : 0 0 10px 0 !important;
    background-color      : white !important;
    border                : 1px solid #DDD !important;
    -webkit-border-radius : 2px !important;
    -moz-border-radius    : 2px !important;
    -ms-border-radius     : 2px !important;
    -o-border-radius      : 2px !important;
    border-radius         : 2px !important;
    box-shadow            : none !important;
    text-decoration       : none !important;
    text-align            : left !important;
  }
  .coursavenue-jpo-header {
    margin       : 0 0 10px 0 !important;
    font-size    : 14px !important;
  }
  .coursavenue-jpo-header a {
    text-decoration: none !important;
  }
  .coursavenue-jpo-btn:hover {
    text-decoration: underline !important;
  }
  .coursavenue-jpo-btn {
    display                  : block !important;
    margin                   : 0 auto 10px 0 !important;
    padding                  : 3px !important;
    background-color         : white !important;
    border                   : 2px solid #e9683d!important;
    color                    : black !important;
    text-align               : center !important;
    -webkit-border-radius    : 2px !important;
    -moz-border-radius       : 2px !important;
    -ms-border-radius        : 2px !important;
    -o-border-radius         : 2px !important;
    border-radius            : 2px !important;
  }

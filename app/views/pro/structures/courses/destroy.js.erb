$('#course-<%= @course.id %>').slideUp();
setTimeout(function() {
    $('#course-<%= @course.id %>').remove();
    if ($("#<%= @course.underscore_name %>-list > div").length == 0) {
        $("[data-empty=<%= @course.underscore_name %>]").slideDown();
    }
}, 500);

<% if @place.persisted? %>
    <% if @place.is_home? %>
        window.location.href = "<%= pro_structure_places_path(@structure) %>";
    <% else %>
        $.fancybox("<%== escape_javascript(render partial: 'pro/structures/places/verify_address') %>", { width: 600, minWidth: 600, maxWidth: 600, padding: 0, helpers: { overlay : { closeClick: false } } });
    <% end %>
<% else %>
    $.fancybox("<%== escape_javascript(render partial: 'pro/structures/places/form') %>", { width: 750, minWidth: 750, maxWidth: 750, padding: 0, helpers: { overlay : { closeClick: false } } });
<% end %>
$.each(GLOBAL.initialize_callbacks, function(i, func) { func(); });
